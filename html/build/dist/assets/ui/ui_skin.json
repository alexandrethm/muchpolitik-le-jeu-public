{
  com.badlogic.gdx.graphics.g2d.BitmapFont: {
    dialog-font: {
      file: fonts/dialog_font.fnt
    }
    ui-font: {
      file: fonts/ui_font.fnt
    }
    title-font: {
      file: fonts/title_font.fnt
    }
  }

  com.badlogic.gdx.graphics.Color: {
    white: {
      a: 1
      b: 1
      g: 1
      r: 1
    }
    red: {
      a: 1
      b: 0
      g: 0
      r: 1
    }
    black: {
      a: 1
      b: 0
      g: 0
      r: 0
    }
    beige: {
      a: 1
      b: 0.75
      g: 0.85
      r: 0.9
    }
    transparent: {
      a: 0.7
      b: 1
      g: 1
      r: 1
    }
  }

  com.badlogic.gdx.scenes.scene2d.ui.Skin$TintedDrawable: {
    stageBackgroundDim: {
      name: white
      color: {
        r: 0
        g: 0
        b: 0
        a: 0.8
      }
    }
    labelBackgroundDim: {
      name: white-round
      color: {
        r: 0
        g: 0
        b: 0
        a: 0.4
      }
    }
    whiteBackground: {
      name: white
      color: white
    }
    arrow-jump-pressed-transparent: {
      name: arrow-jump-pressed
      color: transparent
    }
    arrow-jump-transparent: {
      name: arrow-jump
      color: transparent
    }
    arrow-left-pressed-transparent: {
      name: arrow-left-pressed
      color: transparent
    }
    arrow-left-transparent: {
      name: arrow-left
      color: transparent
    }
    arrow-right-pressed-transparent: {
      name: arrow-right-pressed
      color: transparent
    }
    arrow-right-transparent: {
      name: arrow-right
      color: transparent
    }
  }

  com.badlogic.gdx.scenes.scene2d.ui.Label$LabelStyle: {
    default: {
      font: ui-font
      fontColor: black
    }
    ui-white: {
      font: ui-font
      fontColor: white
    }
    game-ui-white: {
      font: ui-font
      fontColor: white
      background: labelBackgroundDim
    }
    game-ui-red: {
      font: ui-font
      fontColor: red
      background: labelBackgroundDim
    }
    dialog: {
      font: dialog-font
      fontColor: black
    }
    title: {
      font: title-font
      fontColor: black
    }
  }

  com.badlogic.gdx.scenes.scene2d.ui.Button$ButtonStyle: {
    default: {
      down: button-pressed
      up: button
      over: button-hover
    }
  }

  com.badlogic.gdx.scenes.scene2d.ui.TextButton$TextButtonStyle: {
    default: {
      down: button-pressed
      up: button
      over: button-hover
      disabled: button-disabled
      font: ui-font
      fontColor: black
      downFontColor: beige
      disabledFontColor: white
    }
  }

  com.badlogic.gdx.scenes.scene2d.ui.ImageButton$ImageButtonStyle: {
    default: {
      imageUp: button
      imageDown: button-pressed
      imageChecked: button-hover
    }
    arrow-left: {
      imageUp: arrow-left-transparent
      imageDown: arrow-left-pressed-transparent
      imageChecked: arrow-left-pressed-transparent
    }
    arrow-right: {
      imageUp: arrow-right-transparent
      imageDown: arrow-right-pressed-transparent
      imageChecked: arrow-right-pressed-transparent
    }
    arrow-jump: {
      imageUp: arrow-jump-transparent
      imageDown: arrow-jump-pressed-transparent
      imageChecked: arrow-jump-pressed-transparent
    }
    button-pause: {
      up: button-pause
      over: button-pause-hover
      down: button-pause-pressed
    }
  }

  com.badlogic.gdx.scenes.scene2d.ui.ImageTextButton$ImageTextButtonStyle: {
    default: {
      imageDown: basic-level-unlocked-pressed
      imageUp: basic-level-unlocked
      imageDisabled: basic-level-locked
      font: ui-font
      fontColor: black
    }
    basic-level-unlocked: {
      imageDown: basic-level-unlocked-pressed
      imageUp: basic-level-unlocked
      imageDisabled: basic-level-locked
      font: ui-font
      fontColor: white
    }
    basic-level-done: {
      imageDown: basic-level-done-pressed
      imageUp: basic-level-done
      imageDisabled: basic-level-locked
      font: ui-font
      fontColor: white
    }
    boss-level-unlocked: {
      imageDown: boss-level-unlocked-pressed
      imageUp: boss-level-unlocked
      imageDisabled: boss-level-locked
      font: ui-font
      fontColor: white
    }
    boss-level-done: {
      imageDown: boss-level-done-pressed
      imageUp: boss-level-done
      imageDisabled: boss-level-locked
      font: ui-font
      fontColor: white
    }
  }

  com.badlogic.gdx.scenes.scene2d.ui.Window$WindowStyle: {
    default: {
      titleFont: title-font
      titleFontColor: white
      background: window
    }
    dialog: {
      titleFont: title-font
      background: window
      titleFontColor: white
      stageBackground: stageBackgroundDim
    }
    speechBubble: {
      titleFont: title-font
      background: speech-bubble
      titleFontColor: white
    }
  }

  com.badlogic.gdx.scenes.scene2d.ui.ScrollPane$ScrollPaneStyle: {
    default: {
      hScroll: scrollbar-horizontal
      hScrollKnob: scrollbar-horizontal-knob
      vScroll: scrollbar-vertical
      vScrollKnob: scrollbar-vertical-knob
    }
    shop: {
      hScroll: scrollbar-horizontal
      hScrollKnob: scrollbar-horizontal-knob
      vScroll: scrollbar-vertical
      vScrollKnob: scrollbar-vertical-knob
    }
  }

  com.badlogic.gdx.scenes.scene2d.ui.SelectBox$SelectBoxStyle: {
    default: {
      font: ui-font
      fontColor: white
      background: select-box
      scrollStyle: default
      backgroundOver: select-box-over
      backgroundOpen: select-box-pressed
    }
  }

  com.badlogic.gdx.scenes.scene2d.ui.SplitPane$SplitPaneStyle: {
    default-vertical: {
      handle: splitpane-horizontal
    }
    default-horizontal: {
      handle: splitpane-vertical
    }
  }

  com.badlogic.gdx.scenes.scene2d.ui.ProgressBar$ProgressBarStyle: {
    default-horizontal: {
      background: progress-bar-horizontal
      knobBefore: progress-bar-horizontal-knob
    }
    default-vertical: {
      background: progress-bar-vertical
      knobBefore: progress-bar-vertical-knob
    }
  }

  com.badlogic.gdx.scenes.scene2d.ui.Slider$SliderStyle: {
    default-horizontal: {
      knobOver: slider-knob-over
      knobDown: slider-knob-pressed
      background: slider
      knob: slider-knob
    }
    default-vertical: {
      knobOver: slider-knob-over
      knobDown: slider-knob-pressed
      background: slider
      knob: slider-knob
    }
  }

  com.badlogic.gdx.scenes.scene2d.ui.TextField$TextFieldStyle: {
    default: {
      font: ui-font
      fontColor: black
      background: textfield
      focusedBackground: textfield-selected
    }
  }

  com.badlogic.gdx.scenes.scene2d.ui.CheckBox$CheckBoxStyle: {
    default: {
      checkboxOn: checkbox-pressed
      checkboxOff: checkbox
      checkboxOver: checkbox-over
      font: ui-font
      fontColor: black
    }
    radio: {
      checkboxOn: radio-pressed
      checkboxOff: radio
      checkboxOver: radio-over
      font: ui-font
      fontColor: black
    }
  }

  com.badlogic.gdx.scenes.scene2d.ui.List$ListStyle: {
    default: {
      font: ui-font
      fontColorSelected: black
      fontColorUnselected: black
      selection: button-menu-over
      background: list
    }
  }

  com.badlogic.gdx.scenes.scene2d.ui.Touchpad$TouchpadStyle: {
    default: {
      background: progress-bar-circle-knob
      knob: radio
    }
  }

  com.badlogic.gdx.scenes.scene2d.ui.Tree$TreeStyle: {
    default: {
      plus: plus
      minus: minus
    }
  }
}
